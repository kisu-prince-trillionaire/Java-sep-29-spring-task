<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="stu1" class="com.student.bean.Student">
		<property name="rollNo" value="101"></property>
		<property name="name" value="Vishal"></property>
		<property name="salary" value="10000"></property>
	</bean>
	<bean id="exam1" class="com.exam.bean.Exam">
		<property name="eId" value="101"></property>
		<property name="name" value="Java"></property>
		<property name="marks" value="100"></property>
	</bean>


</beans>